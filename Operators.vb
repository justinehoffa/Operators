Module Operators

    Sub Main()
        'Let's do math!!
        'This is a comment 
        Console.WriteLine(2 + 2)
        Console.ReadLine()
        'Subtraction
        Console.WriteLine(7 - 1)
        Console.ReadLine()
        'Multiplication
        Console.WriteLine(5 * 5)
        Console.ReadLine()
        'Normal Division
        Console.WriteLine(7 / 3)
        Console.ReadLine()
        'Integer Division
        Console.WriteLine(7 \ 3)
        Console.ReadLine()
        'Mod/ Remainder
        Console.WriteLine(7 Mod 3)
        Console.ReadLine()
    End Sub

End Module
